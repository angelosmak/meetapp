<%= simple_form_for(@profile) do |f| %>
  <%= f.input :username, input_html: { value: current_user.email.to_s.split('@')[0] } %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :photo, as: :file %>
  <%= f.input :bio %>
  <%= f.input :preferences, collection: ['sport', 'music', 'food', 'movies', 'outdoor'], as: :check_boxes %>
  <%= f.input :address,
        input_html: {data: {address_autocomplete_target: "address"},class: "d-none"},
        wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}} %>  <%= f.submit %>
<% end %>
